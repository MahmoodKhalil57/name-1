  "version": "0.2.0",
  "configurations": [
    {
      "command": "quasar dev",
      "name": "quasar dev",
      "request": "launch",
      "type": "node-terminal"
    },
    {
      "type": "chrome",
      "request": "launch",
      "name": "quasar debug",
      "url": "http://localhost:9000",
      "webRoot": "${workspaceFolder}/src",
      "sourceMapPathOverrides": {
        "webpack://package-name/./src/*": "${webRoot}/*"
      }
    }
  ]
}
